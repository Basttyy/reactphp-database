    "keywords": ["async", "ReactPHP", "ORM", "Database"],
    "license": "MIT",
    "authors": [
        {
            "name": "Abdulbasit Mamman",
            "homepage": "https://github.com/Basttyy",
            "email": "basttyydev@gmail.com"
        }
